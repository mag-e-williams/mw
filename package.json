{
  "name": "mw",
  "version": "0.0.0-development",
  "description": "Margret Williams's public website, implemented in NextJS",
  "repository": "https://github.com/mag-e-williams/mw.git",
  "homepage": "https://github.com/mag-e-williams/mw#readme",
  "author": "Margret Williams <margretwilliams@42@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "dev": "VERCEL_GIT_COMMIT_SHA=$(git rev-parse HEAD) next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "serve": "next start",
    "release": "semantic-release",
    "format": "prettier --loglevel warn --write \"**/*.{js,ts,tsx,mjs,cjs,css,md,json,yml,yaml}\"",
    "lint": "eslint . --cache --fix --ext .js,.ts,.tsx,.mjs,.cjs --no-eslintrc -c config/eslint.config.js",
    "lint:types": "tsc",
    "codegen": "DOTENV_CONFIG_PATH='.env.development.local' graphql-codegen --require dotenv/config --config codegen.ts",
    "db": "sequelize",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:status": "sequelize db:migrate:status",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:generate": "sequelize migration:generate",
    "db:connect": "pscale connect mw --port 3306",
    "clean": "rm -r .next"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "15.16.2",
    "@contentful/rich-text-types": "16.0.2",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-brands-svg-icons": "6.2.1",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@mui/lab": "5.0.0-alpha.129",
    "@mui/material": "5.10.16",
    "@next/bundle-analyzer": "13.0.6",
    "@uiball/loaders": "1.2.6",
    "@vercel/og": "0.0.21",
    "animate-css-grid": "1.4.3",
    "aws-sdk": "2.1362.0",
    "axios": "1.4.0",
    "cmd-ts": "0.11.0",
    "dotenv": "16.0.3",
    "eslint-config-mw": "link:./eslint-plugin-mw",
    "eslint-plugin-mw": "link:./eslint-plugin-mw",
    "eventemitter3": "5.0.1",
    "graphql": "16.6.0",
    "graphql-request": "5.0.0",
    "husky": "8.0.3",
    "lucide-react": "0.104.1",
    "mapbox-gl": "2.11.0",
    "material-ui-image": "3.3.2",
    "micromatch": "4.0.5",
    "mysql2": "2.3.3",
    "next": "13.0.6",
    "node-fetch": "3.3.0",
    "react": "18.2.0",
    "react-adobe-embed": "3.69.666",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.4.1",
    "react-is": "18.2.0",
    "react-map-gl": "7.0.20",
    "react-progressive-graceful-image": "0.7.0",
    "reflect-metadata": "0.1.13",
    "rss-parser": "3.13.0",
    "sequelize": "6.26.0",
    "sequelize-typescript": "2.1.5",
    "sharp": "0.31.2",
    "swr": "2.1.5",
    "use-breakpoint": "3.0.5",
    "webpack": "5.75.0",
    "xml2js": "0.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.15.0",
    "@graphql-codegen/near-operation-file-preset": "2.4.4",
    "@graphql-codegen/typescript": "2.8.3",
    "@graphql-codegen/typescript-operations": "2.5.8",
    "@semantic-release/changelog": "6.0.2",
    "@types/jsdom": "21.1.1",
    "@types/mapbox-gl": "2.7.10",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.11.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/validator": "13.7.10",
    "@types/xml2js": "0.4.11",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "@typescript-eslint/parser": "5.45.1",
    "eslint": "8.29.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "lint-staged": "13.1.0",
    "postcss": "8.4.19",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.0",
    "semantic-release": "19.0.5",
    "sequelize-cli": "6.5.2",
    "tsm": "2.3.0",
    "turbo": "1.6.3",
    "typescript": "4.9.3"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@7.18.1"
}
